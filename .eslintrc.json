{
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "extends": [
        "plugin:vue/vue3-recommended"
    ],
    "rules": {
        "vue/no-unused-vars": "error",
        "vue/v-on-event-hyphenation": 0,
        "vue/multi-word-component-names": 0,
        "quotes": [
            "error",
            "single"
        ]
    }
}